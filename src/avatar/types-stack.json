{"index.html":"<html><head><script src=\"//npmci.syncfusion.com/development/demos/avatar/types/datasource.js\" type=\"text/javascript\"></script>\n<script src=\"https://cdn.syncfusion.com/ej2/dist/ej2.min.js\" type=\"text/javascript\"></script>\n    <link href=\"https://cdn.syncfusion.com/ej2/material.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\"><div class=\"col-lg-12 control-section\">\n    <div class=\"sample_container avatar-types\">\n        <div class=\"avatar-block\">\n            <!-- Card Component -->\n            <div class=\"e-card e-avatar-showcase\">\n                <div class=\"e-card-content\">\n                    <!-- XLarge Circle Avatar Component -->\n                    <div class=\"e-avatar e-avatar-xlarge e-avatar-circle\">\n                        <img class=\"image\" src=\"//npmci.syncfusion.com/development/demos/src/avatar/images/pic01.png\" alt=\"avatar\">\n                    </div>\n                </div>\n                <div class=\"e-card-content\">\n                    <div>Image</div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"avatar-block\">\n            <!-- Card Component -->\n            <div class=\"e-card e-avatar-showcase\">\n                <div class=\"e-card-content\">\n                    <!-- XLarge Circle Avatar Component -->\n                    <div class=\"e-avatar e-avatar-xlarge e-avatar-circle\">\n                        <div class=\"svg_icons chrome\"></div>\n                    </div>\n                </div>\n                <div class=\"e-card-content\">\n                    <div>SVG</div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"avatar-block\">\n            <!-- Card Component -->\n            <div class=\"e-card e-avatar-showcase\">\n                <div class=\"e-card-content\">\n                    <!-- XLarge Circle Avatar Component -->\n                    <div class=\"e-avatar e-avatar-xlarge e-avatar-circle\">GR</div>\n                </div>\n                <div class=\"e-card-content\">\n                    <div>Initial</div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"avatar-block\">\n            <!-- Card Component -->\n            <div class=\"e-card e-avatar-showcase\">\n                <div class=\"e-card-content\">\n                    <!-- XLarge Circle Avatar Component -->\n                    <div class=\"e-avatar e-avatar-xlarge e-avatar-circle\">\n                        <div class=\"e-people icons\"></div>\n                    </div>\n                </div>\n                <div class=\"e-card-content\">\n                    <div>Font Icon</div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"avatar-block\">\n            <!-- Card Component -->\n            <div class=\"e-card e-avatar-showcase\">\n                <div class=\"e-card-content\">\n                    <!-- XLarge Circle Avatar Component -->\n                    <div class=\"e-avatar e-avatar-xlarge e-avatar-circle\">User</div>\n                </div>\n                <div class=\"e-card-content\">\n                    <div>Word</div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"avatar-block\">\n            <!-- Card Component -->\n            <div class=\"e-card e-avatar-showcase\">\n                <div class=\"e-card-content\">\n                    <!-- XLarge Circle Avatar Component -->\n                    <div class=\"e-avatar e-avatar-xlarge e-avatar-circle custom\">\n                        <div class=\"e-people icons\"></div>\n                    </div>\n                </div>\n                <div class=\"e-card-content\">\n         