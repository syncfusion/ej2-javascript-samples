{"index.html":"<html><head><script src=\"http://cdn.syncfusion.com/ej2/dist/ej2.min.js\" type=\"text/javascript\"></script>\n    <link href=\"http://cdn.syncfusion.com/ej2/material.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\"><div class=\"col-lg-12 control-section inplace-editor-control-section form-layout\">\n    <div class=\"content-wrapper\" style=\"margin-bottom: 25px\">\n        <div id=\"confirmation\">\n            <form id=\"formId\" class=\"form-horizontal\">\n                <div class=\"form-group\">\n                    <label class=\"col-sm-6 control-label\">\n                        TextBox </label>\n                    <div class=\"col-sm-6\">\n                        <div style=\"display:none\">\n                            <input type=\"text\" id=\"textInput\" value=\"Janat\" style=\"width:110px;\">\n                        </div>\n                        <div id=\"inplace_editor\"></div>\n                    </div>\n                </div>\n                <div class=\"form-group\">\n                    <label class=\"col-sm-6 control-label\">\n                        Date </label>\n                    <div class=\"col-sm-6\">\n                        <div style=\"display:none\">\n                            <input type=\"date\" id=\"dateInput\" value=\"2013-01-08\" style=\"width:110px;\" \">\n                        </div>\n                        <div id=\"inplace_editor_date\"></div>\n                    </div>\n                </div>\n                <div class=\"form-group\">\n                    <label class=\"col-sm-6 control-label\">\n                        Select </label>\n                    <div class=\"col-sm-6\">\n                        <div style=\"display:none\">\n                            <select id=\"selectInput\" style=\"width:110px;height:25px\">\n                                <option value=\"Volvo\">Volvo</option>\n                                <option value=\"Saab\">Saab</option>\n                                <option value=\"Fiat\">Liat</option>\n                                <option value=\"Audi\">Audi</option>\n                            </select>\n                        </div>\n                        <div id=\"inplace_editor_select\"></div>\n                    </div>\n                </div>\n            </form>\n        </div>\n    </div>\n</div>\n\n\n<style>\n    .inplace-editor-control-section #confirmation {\n        max-width: 550px;\n        margin: auto;\n    }\n\n    .highcontrast .inplace-editor-control-section #confirmation input {\n        background: inherit;\n    }\n\n    .highcontrast .inplace-editor-control-section #confirmation select,\n    .highcontrast .inplace-editor-control-section #confirmation select option {\n        background: #000;\n    }\n\n    .inplace-editor-control-section #formId {\n        padding-top: 20px;\n        margin-bottom: 45px;\n        border: 1px solid #ccc;\n        box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.36);\n        border-radius: 5px;\n    }\n\n    .inplace-editor-control-section .form-horizontal .form-group {\n        margin: 0 20px;\n        padding-top: 11px;\n        padding-bottom: 35px\n    }\n\n    .inplace-editor-control-section.form-layout .control-wrapper .form-horizontal .control-label {\n        text-align: center;\n        font-size: 14px;\n        padding-left: 0px;\n        font-weight: 400;\n    }\n\n    .inplace-editor-control-section .form-horizontal .form-group:last-child {\n        padding-bottom: 40px;\n    }\n\n    @media (max-width: 768px) {\n        .inplace-editor-control-section.form-layout .control-wrapper .form-horizontal .control-label {\n            width: 50%;\n            float: left;\n            padding-top: 7px;\n            margin-bottom: 0;\n        }\n\n        .inplace-editor-control-section.form-layout .control-wrapper .form-horizontal .control-label~div {\n            width: 50%;\n            float: left;\n        }\n    }\n</style></div></body></html>","index.js":"{{ripple}}\n    var editObj = new ej.inplaceeditor.InPlaceEditor({\n        mode: 'Inline',\n        template: '#textInput',\n        value: 'Janat',\n        actionBegin: actionTextBegin\n    });\n    editObj.appendTo('#inplace_editor');\n    var dateObj = new ej.inplaceeditor.InPlaceEditor({\n        mode: 'Inline',\n        template: '#dateInput',\n        value: '2013-01-08',\n        actionBegin: actionDateBegin\n    });\n    dateObj.appendTo('#inplace_editor_date');\n    var selectObj = new ej.inplaceeditor.InPlaceEditor({\n        mode: 'Inline',\n        value: 'Volvo',\n        template: '#selectInput',\n        actionBegin: actionSelectBegin\n    });\n    selectObj.appendTo('#inplace_editor_select');\n    function actionTextBegin(e) {\n        editObj.value = document.getElementById('textInput').value;\n    }\n    function actionDateBegin(e) {\n        dateObj.value = document.getElementById('dateInput').value;\n    }\n    function actionSelectBegin(e) {\n        selectObj.value = document.getElementById('selectInput').value;\n    }\n"}