{"index.html":"<html><head><script src=\"https://cdn.syncfusion.com/ej2/dist/ej2.min.js\" type=\"text/javascript\"></script>\n    <link href=\"https://cdn.syncfusion.com/ej2/material.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\"><div class=\"col-lg-12 control-section\">\n    <div class=\"content-wrapper\">\n        <div class=\"sliderwrap\">\n            <label class=\"labeltext userselect\">Default Slider</label>\n            <div id=\"default\"></div>\n        </div>\n        <div class=\"sliderwrap\">\n            <label class=\"labeltext userselect\">MinRange Slider</label>\n            <div id=\"minrange\"></div>\n        </div>\n        <div class=\"sliderwrap\">\n            <label class=\"labeltext userselect\">Range Slider</label>\n            <div id=\"range\"></div>\n        </div>\n    </div>\n</div>\n\n\n\n\n\n<style>\n    .content-wrapper {\n        width: 40%;\n        margin: 0 auto;\n        min-width: 185px;\n    }\n\n    .sliderwrap {\n        margin-top: 40px;\n    }\n\n    .e-bigger .content-wrapper {\n        width: 80%;\n    }\n\n    .sliderwrap label {\n        padding-bottom: 26px;\n        font-size: 13px;\n        font-weight: 500;\n        margin-top: 15px;\n    }\n\n    .userselect {\n        -webkit-user-select: none;\n        /* Safari 3.1+ */\n        -moz-user-select: none;\n        /* Firefox 2+ */\n        -ms-user-select: none;\n        /* IE 10+ */\n        user-select: none;\n        /* Standard syntax */\n    }\n</style></div></body></html>","index.js":"{{ripple}}\r\n    var defaultObj = new ej.inputs.Slider({\r\n        value: 30\r\n    });\r\n    defaultObj.appendTo('#default');\r\n    var minRangeObj = new ej.inputs.Slider({\r\n        value: 30,\r\n        type: 'MinRange'\r\n    });\r\n    minRangeObj.appendTo('#minrange');\r\n    var rangeObj = new ej.inputs.Slider({\r\n        value: [30, 70],\r\n        type: 'Range'\r\n    });\r\n    rangeObj.appendTo('#range');\r\n"}