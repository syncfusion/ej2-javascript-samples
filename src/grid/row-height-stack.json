{"index.html":"<html><head><script src=\"//npmci.syncfusion.com/development/demos/grid/row-height/datasource.js\" type=\"text/javascript\"></script>\n<script src=\"https://cdn.syncfusion.com/ej2/dist/ej2.min.js\" type=\"text/javascript\"></script>\n    <link href=\"https://cdn.syncfusion.com/ej2/material.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\"><style>\n@font-face {\nfont-family: 'e-grid-rowheight';\nsrc:\nurl(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\nfont-weight: normal;\nfont-style: normal;\n}\n.e-grid .e-small-icon::before {\n    font-family: 'e-grid-rowheight';\n    content: '\\e707';\n}\n\n.e-grid .e-medium-icon::before {\n    font-family: 'e-grid-rowheight';\n    content: '\\e701';\n}\n\n.e-grid .e-big-icon::before {\n    font-family: 'e-grid-rowheight';\n    content: '\\e702';\n}\n</style>\n<div>\n    <div class=\"control-section\">\n        <div class=\"content-wrapper\">\n            <div id=\"Grid\"></div>\n        </div>\n    </div>\n</div>\n\n\n\n\n</div></body></html>","index.js":"{{ripple}}/**\r\n * Row Height sample\r\n */\r\n\r\n    var gridData = new ej.data.DataManager(window.data).executeLocal(new ej.data.Query().take(30));\r\n    var grid = new ej.grids.Grid({\r\n        dataSource: gridData,\r\n        toolbar: [\r\n            { prefixIcon: 'e-small-icon', id: 'big', align: 'right' },\r\n            { prefixIcon: 'e-medium-icon', id: 'medium', align: 'right' },\r\n            { prefixIcon: 'e-big-icon', id: 'small', align: 'right' }\r\n        ],\r\n        columns: [\r\n            { field: 'OrderID', headerText: 'Order ID', width: 120, textAlign: 'Right' },\r\n            { field: 'CustomerName', headerText: 'Customer Name', width: 150 },\r\n            { field: 'OrderDate', headerText: 'Order Date', width: 130, format: 'yMd', textAlign: 'Right' },\r\n            { field: 'Freight', width: 120, format: 'C2', textAlign: 'Right' },\r\n            { field: 'ShippedDate', headerText: 'Shipped Date', width: 140, format: 'yMd', textAlign: 'Right' },\r\n            { field: 'ShipCountry', headerText: 'Ship Country', width: 150 }\r\n        ],\r\n        height: 400,\r\n        rowHeight: 20,\r\n        toolbarClick: function (args) {\r\n            if (args.item.id === 'small') {\r\n                grid.rowHeight = 20;\r\n            }\r\n            if (args.item.id === 'medium') {\r\n                grid.rowHeight = 40;\r\n            }\r\n            if (args.item.id === 'big') {\r\n                grid.rowHeight = 60;\r\n            }\r\n        }\r\n    });\r\n    grid.appendTo('#Grid');\r\n"}